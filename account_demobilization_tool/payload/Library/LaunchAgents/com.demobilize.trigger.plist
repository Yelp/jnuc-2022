<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.demobilize.trigger</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/zsh</string>
		<string>-c</string>
		<string>echo "Running at: $(date)" >>| /var/tmp/.localhost.run_demobilize</string>
	</array>
  <key>RunAtLoad</key>
  <true/>
</dict>
</plist>
